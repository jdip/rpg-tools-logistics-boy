{
  "name": "rpg-tools-logistics-boy",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run watch\" \"npm run cy\"",
    "dev-server": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cy": "npx cypress open --e2e --browser chrome",
    "watch": "chokidar --initial \"src/**/*\" -c \"npm run build\""
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@league-of-foundry-developers/foundry-vtt-types": "^9.280.0",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.3.0",
    "cypress-vite": "^1.3.0",
    "cypress-watch-and-reload": "^1.10.5",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^4.0.0"
  }
}
