{
  "name": "rpg-tools-logistics-boy",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run cy-wait\"  ",
    "dev-server": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode=production",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --mode=development",
    "cy": "npx cypress open --e2e --browser chrome",
    "cy-wait": "wait-on -w 2000 dist/styles/rpg-tools-logistics-boy.css dist/module.json dist/scripts/rpg-tools-logistics-boy.js dist/scripts/rpg-tools-logistics-boy.js.map && npm run cy",
    "test": "npx cypress run --e2e --browser chrome",
    "clean": "node utils/clean.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/webpack-preprocessor": "^5.16.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "cypress": "^12.4.0",
    "del": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "istanbul-lib-coverage": "^3.2.0",
    "nyc": "^15.1.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
